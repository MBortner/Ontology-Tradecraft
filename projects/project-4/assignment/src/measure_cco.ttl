#################################################################
### BOILER-07 MEASUREMENTS & QUALITIES
#################################################################

ex:Boiler-07_pressure_Measurement_1 a cco:Measurement ;
    rdfs:label "Boiler-07 pressure measurement 1" ;
    cco:has_data_value "101.325"^^xsd:float ;
    cco:has_unit ex:Unit_kPa_gauge ;
    cco:measurement_time "2024-03-17T23:04:00+00:00"^^xsd:dateTime .

ex:Boiler-07_pressure_Quality a cco:Quality ;
